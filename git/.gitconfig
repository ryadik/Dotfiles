[includeIf "gitdir:~/corp/"]
    path = ~/.dotfiles/git/.corp.gitconfig
[core]
  quotepath = false
  excludesfile = ~/.gitignore_global
  editor = /usr/local/bin/nvim
[color]
  ui = true
  branch = auto
  diff = auto
  status = auto
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
[color "status"]
  added = yellow
  changed = green
  untracked = cyan
; [branch]
;   autosetuprebase = always
[alias]
  cleanup = "!git branch --merged | grep  -v '\\*\\|master\\|develop' | xargs -n 1 git branch -d"
[push]
  default = simple
